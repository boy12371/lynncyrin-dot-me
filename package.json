{
  "name": "whatslynncoding",
  "version": "0.0.0",
  "engines": {
    "node": "10.4.0",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "concurrently -k -n angular,express,storybook -c red,green,yellow \"npm run start:angular\" \"npm run start:express\" \"npm run start:storybook\"",
    "start:angular": "ng serve --port 8080 --open",
    "start:storybook": "start-storybook -s ./angular -c storybook --port 9001",
    "start:express": "nodemon --exec ts-node server.ts",
    "postinstall": "make download-resources",
    "heroku-start": "node server.js",
    "heroku-postbuild": "npm run build:angular && npm run build:express && npm run build:storybook",
    "build:angular": "ng build --prod",
    "build:express": "tsc server.ts",
    "build:storybook": "build-storybook -s ./angular -c storybook -o storybook-dist"
  },
  "dependencies": {
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "body-parser": "^1.18.3",
    "d3": "^5.4.0",
    "dotenv": "^5.0.1",
    "express": "^4",
    "make-fetch-happen": "^4.0.1",
    "markdown-loader": "^2.0.2",
    "marked": "^0.3.19",
    "node-gyp": "^3.7.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1",
    "@angular/compiler-cli": "^5.2.11",
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/angular": "^3.4.6",
    "@types/express": "^4.16.0",
    "@types/node": "^10",
    "@types/node-fetch": "^2",
    "babel-core": "^6.26.3",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "<2.7"
  }
}
