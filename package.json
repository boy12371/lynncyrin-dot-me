{
  "name": "whatslynncoding",
  "version": "0.0.0",
  "engines": {
    "node": "9.5.0",
    "npm": "5.8.0"
  },
  "scripts": {
    "start": "concurrently -k -n angular,express,storybook -c red,green,yellow \"npm run start:angular\" \"npm run start:express\" \"npm run start:storybook\"",
    "start:angular": "ng serve --port 8080 --open",
    "start:storybook": "start-storybook -s ./angular -c storybook --port 9001",
    "start:express": "nodemon --exec ts-node server.ts",
    "postinstall": "make download-resources",
    "heroku-start": "node server.js",
    "heroku-postbuild": "npm run build:angular && npm run build:express && npm run build:storybook",
    "build:angular": "ng build --prod",
    "build:express": "tsc server.ts",
    "build:storybook": "build-storybook -s ./angular -c storybook -o storybook-dist"
  },
  "dependencies": {
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "body-parser": "^1.18.2",
    "d3": "^5.1.0",
    "express": "^4",
    "make-fetch-happen": "^4.0.1",
    "markdown-loader": "^3.0.0",
    "marked": "^0.4.0",
    "dotenv": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addons": "^3.4.2",
    "@storybook/angular": "^3.4.2",
    "@types/express": "^4.11.1",
    "@types/node": "^10.3.1",
    "@types/node-fetch": "^2.1.1",
    "babel-core": "^6.26.0",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "2.9.1"
  }
}
