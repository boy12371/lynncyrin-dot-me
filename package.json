{
  "name": "whatslynncoding",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently -k -n angular,express,storybook -c red,green,yellow \"npm run start:angular\" \"npm run start:express\" \"npm run start:storybook\"",
    "start:angular": "ng serve",
    "start:storybook": "cp src/index.html storybook/preview-head.html && start-storybook -c storybook -p 9001",
    "start:express": "nodemon --exec ts-node --typeCheck server.ts",
    "heroku-start": "node server.js",
    "heroku-postbuild": "npm run build:angular && npm run build:express && npm run build:storybook",
    "build:angular": "ng build --aot -prod --progress --sourcemaps",
    "build:express": "tsc server.ts",
    "build:storybook": "build-storybook -c storybook -o storybook-dist"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5",
    "@angular/compiler": "^5",
    "@angular/core": "^5",
    "@angular/forms": "^5",
    "@angular/http": "^5",
    "@angular/platform-browser": "^5",
    "@angular/platform-browser-dynamic": "^5",
    "@angular/router": "^5",
    "body-parser": "^1.18.2",
    "d3": "^5.0.0",
    "make-fetch-happen": "^4.0.0",
    "zone.js": "^0.8.26",
    "express": "^4"
  },
  "devDependencies": {
    "@angular/cli": "^1",
    "@angular/compiler-cli": "^5",
    "@storybook/addon-actions": "^3.4",
    "@storybook/addons": "^3.4",
    "@storybook/angular": "^3.4",
    "@types/express": "^4.11.1",
    "@types/jasmine": "^2",
    "@types/node": "^9",
    "@types/node-fetch": "^1.6.8",
    "dotenv": "^5.0.1",
    "babel-core": "^6.26.0",
    "jasmine-core": "^3",
    "jasmine-spec-reporter": "^4",
    "karma": "^2",
    "karma-chrome-launcher": "^2",
    "karma-cli": "^1",
    "karma-jasmine": "^1",
    "karma-remap-istanbul": "0.x",
    "nodemon": "^1.17.3",
    "protractor": "^5",
    "ts-node": "^5.0.1",
    "tslint": "^5",
    "typescript": "<2.7"
  }
}
